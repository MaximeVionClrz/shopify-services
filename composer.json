{
    "name": "clrz/shopify-services",
    "source": {
        "url": "git@gitlab.colorz.fr:colorz/shopify-services.git",
        "type": "git",
        "reference": "main"
    },
    "version": "1.0.4",
    "description": "A collection of services using Shopify Php SDK to work with Shopify Store and simplify common action",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Clrz\\ShopifyServices\\": "src/Service"
        }
    },
    "authors": [
        {
            "name": "Maxime Vion",
            "email": "maxime.vion@colorz.fr"
        }
    ],
    "require": {
        "php": "8.0.*",
        "phpclassic/php-shopify": "^1.1"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false
        }
    }
}
